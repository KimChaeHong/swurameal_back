<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper
	namespace="com.company.swurameal.dao.mybatis.CategoryDao">
	<select id="selectCategory" parameterType="int" resultType="Goods">
			select category
			from goods
			
	</select>
	
	<select id="getGoods" parameterType="String" resultType="Goods">
		select category, goodsName, goodsComment, price
		from goods
		where category = #{category}
	</select>

</mapper>
