<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper 
	namespace="com.company.swurameal.dao.mybatis.FaqDao">
	
	<select id="getFaq" resultType="Faq">
		<!-- <![CDATA[
	
			SELECT FAQ_ID, USER_ID, FAQ_TITLE, FAQ_CONTENT, FAQ_REGISTER_DATE
	        FROM (
	        	SELECT rownum as rnum, FAQ_ID, USER_ID, FAQ_TITLE, FAQ_CONTENT, FAQ_REGISTER_DATE	        
		        FROM (
		       		SELECT FAQ_ID, USER_ID, FAQ_TITLE, FAQ_CONTENT, FAQ_REGISTER_DATE	        			        
			        FORM faq
			        ORDER BY FAQ_ID DESC
			        )
			        WHERE rownum <= #{endRowNo}	
		        )
		        WHERE rnum >= #{startRowNo}
        ]]> -->
        
        
		SELECT FAQ_ID,
               USER_ID,
               FAQ_TITLE,
               FAQ_CONTENT,
               FAQ_REGISTER_DATE
        FROM faq	
	</select>
	
</mapper>	
	